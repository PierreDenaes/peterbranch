{% extends 'base.html.twig' %}

{% block title %}PostLike index{% endblock %}

{% block body %}
    <h1>PostLike index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>IsActive</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for post_like in post_likes %}
            <tr>
                <td>{{ post_like.id }}</td>
                <td>{{ post_like.isActive ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_post_like_show', {'id': post_like.id}) }}">show</a>
                    <a href="{{ path('app_post_like_edit', {'id': post_like.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_post_like_new') }}">Create new</a>
{% endblock %}
